local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Az Speed Legends", HidePremium = false, IntroEnabled = true, SaveConfig = true, ConfigFolder = "test"})
local rootPart = game.Players.LocalPlayer.Character.HumanoidRootPart

-- Values
-- city
_G.red1 = true
_G.gem1 = true
_G.xp1 = true
_G.blue1 = true
_G.orange1 = true
_G.openCity = true

-- desert
_G.red2 = true
_G.blue2 = true
_G.orange2 = true
_G.xp2 = true
_G.gem2 = true
_G.autoOpenDesert = true

-- misc
_G.autoRebirth = true
_G.rings = true

-- all features
function red1()
    while _G.red1 == true do
        game:GetService("ReplicatedStorage").rEvents.orbEvent:FireServer("collectOrb", "Red Orb", "City")
        wait(0.00000000000000000001)
    end
end

function gem1()
    while _G.gem1 do
        game:GetService("ReplicatedStorage").rEvents.orbEvent:FireServer("collectOrb", "Gem", "City")
        wait(0.00000000000000000001)
    end
end

function xp1()
    while _G.xp1 do
        game:GetService("ReplicatedStorage").rEvents.orbEvent:FireServer("collectOrb", "Yellow Orb", "City")
        wait(0.00000000000000000001)
    end
end

function orange1()
    while _G.orange1 do
        game:GetService("ReplicatedStorage").rEvents.orbEvent:FireServer("collectOrb", "Orange Orb", "City")
        wait(0.00000000000000000001)
    end
end

function blue1()
    while _G.blue1 do
        game:GetService("ReplicatedStorage").rEvents.orbEvent:FireServer("collectOrb", "Blue Orb", "City")
        wait(0.00000000000000000001)
    end
end

function red2()
    while _G.red2 == true do
        game:GetService("ReplicatedStorage").rEvents.orbEvent:FireServer("collectOrb", "Red Orb", "Desert")
        wait(0.00000000000000000001)
    end
end

function orange2()
    while _G.orange2 == true do
        game:GetService("ReplicatedStorage").rEvents.orbEvent:FireServer("collectOrb", "Orange Orb", "Desert")
        wait(0.00000000000000000001)
    end
end

function blue2()
    while _G.blue2 == true do
        game:GetService("ReplicatedStorage").rEvents.orbEvent:FireServer("collectOrb", "Blue Orb", "Desert")
        wait(0.00000000000000000001)
    end
end

function xp2()
    while _G.xp2 == true do
        game:GetService("ReplicatedStorage").rEvents.orbEvent:FireServer("collectOrb", "Yellow Orb", "Desert")
        wait(0.00000000000000000001)
    end
end

function gem2()
    while _G.gem2 == true do
        game:GetService("ReplicatedStorage").rEvents.orbEvent:FireServer("collectOrb", "Gem", "Desert")
        wait(0.00000000000000000001)
    end
end

function autoOpenDesert()
    while _G.autoOpenDesert == true do
        game:GetService("ReplicatedStorage").rEvents.openCrystalRemote:InvokeServer("openCrystal", "Electro Crystal")
        wait(0.00000000000000000001)
    end
end

function autoRebirth()
    while _G.autoRebirth do
        game:GetService("ReplicatedStorage").rEvents.rebirthEvent:FireServer("rebirthRequest")
        wait(0.05)
    end
end

-- ring tp system
function rings()
    while _G.rings == true do
        for _, hoop in ipairs(workspace.Hoops:GetChildren()) do
            rootPart.CFrame = hoop.CFrame
            wait(0.1)
            rootPart.CFrame = workspace.SpawnLocation.CFrame
            wait(0.5)
        end
    end
end

local cityTab = Window:MakeTab({
    Name = "City Farm",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

cityTab:AddToggle({
    Name = "Red Orb",
    Default = false,
    Callback = function(Value)
        _G.red1 = Value
        red1()
    end
})

cityTab:AddToggle({
    Name = "Orange Orb",
    Default = false,
    Callback = function(Value)
        _G.orange1 = Value
        orange1()
    end
})

cityTab:AddToggle({
    Name = "Blue Orb",
    Default = false,
    Callback = function(Value)
        _G.blue1 = Value
        blue1()
    end
})

cityTab:AddToggle({
    Name = "Auto Gem Farm",
    Default = false,
    Callback = function(Value)
        _G.gem1 = Value
        gem1()
    end
})

cityTab:AddToggle({
    Name = "Auto XP Farm",
    Default = false,
    Callback = function(Value)
        _G.xp1 = Value
        xp1()
    end
})

